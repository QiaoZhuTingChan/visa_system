<language-addon>
	<!-- The name of this addon. It must be unique -->
	<addon-name>BMS</addon-name>

	<!-- Which language this addon will be added to -->
	<language-name>xul/html</language-name>

	<!-- you can specify addition JavaScript files <javascript src="/js/my.js" 
		charset="UTF-8"/> -->

	<!-- an example <component> <component-name>window</component-name> <extends>window</extends> 
		<property> <property-name>border</property-name> <property-value>normal</property-value> 
		</property> </component> -->

	<!-- a macro component <component> <component-name>whatever</component-name> 
		<macro-uri>/WEB-INF/zul/whatever.zul</macro-uri> <component-class>foo.Whatever</component-class> 
		</component> -->
	<component>
		<extends>listbox</extends>
		<component-name>listbox</component-name>
		<component-class>com.jyd.bms.tool.zk.Listbox
		</component-class>
	</component>
	<component>
		<extends>bandbox</extends>
		<component-name>menuTreeBandbox</component-name>
		<component-class>com.jyd.bms.window.admin.component.MenuTreeBandbox
		</component-class>
	</component>
	<component>
		<extends>bandbox</extends>
		<component-name>productCategoryChoose</component-name>
		<component-class>com.jyd.bms.components.ProductCategoryChoose
		</component-class>
	</component>
	<component>
		<extends>listbox</extends>
		<component-name>baPositionListbox</component-name>
		<component-class>com.jyd.bms.components.BaPositionListbox
		</component-class>
	</component>
	<component>
		<extends>listbox</extends>
		<component-name>baModelTypeListbox</component-name>
		<component-class>com.jyd.bms.components.BaModelTypeListbox
		</component-class>
	</component>
	<component>
		<extends>listbox</extends>
		<component-name>baAddressListbox</component-name>
		<component-class>com.jyd.bms.components.BaAddressListbox
		</component-class>
	</component>
	<component>
		<extends>listbox</extends>
		<component-name>baVisaTypeListbox</component-name>
		<component-class>com.jyd.bms.components.BaVisaTypeListbox
		</component-class>
	</component>

	<component>
		<extends>listbox</extends>
		<component-name>baAdmissibleAreaListbox</component-name>
		<component-class>com.jyd.bms.components.BaAdmissibleAreaListbox
		</component-class>
	</component>
</language-addon>
